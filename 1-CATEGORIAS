<script type="text/javascript">
	atOptions = {
		'key' : 'bfa568637f71decff736540c584a3211',
		'format' : 'iframe',
		'height' : 250,
		'width' : 300,
		'params' : {}
	};
	document.write('<scr' + 'ipt type="text/javascript" src="//www.topcreativeformat.com/bfa568637f71decff736540c584a3211/invoke.js"></scr' + 'ipt>');
</script>


  </div>

  <script>
    window.addEventListener('DOMContentLoaded', function() {
      var overlay = document.getElementById('overlay');
      var popup = document.getElementById('popup');
      var countdown = document.getElementById('countdown');
      var timeLeft = 8;

      overlay.style.display = 'block';
      popup.style.display = 'block';

      var countdownInterval = setInterval(function() {
        timeLeft--;
        countdown.textContent = timeLeft;

        if (timeLeft <= 0) {
          clearInterval(countdownInterval);
          overlay.style.display = 'none';
          popup.style.display = 'none';
        }
      }, 1000);
    });
  </script>

  <style>
    #overlay {
      display: none;
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.5);
      backdrop-filter: blur(2px); /* Efecto de difuminado */
      z-index: 9999;
    }

    #popup {
      display: none;
      position: fixed;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      width: 400px; /* Ancho del cuadro ajustado a 400px */
      padding: 20px;
      background-color: white; /* Fondo blanco */
      border: 1px solid #000000;
      box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.5);
      text-align: center;
      z-index: 10000;
    }

    #countdown {
      font-size: 24px;
      margin-top: 10px;
      color: black;
    }
  </style>
